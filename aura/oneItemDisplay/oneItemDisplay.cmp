<aura:component >

    <aura:attribute name="campingListItem" type="Camping_Item__c"/>

    <p>Name:
        <ui:outputText value="{!v.campingListItem.Name}"/>
    </p>
    <p>Quantity:
        <ui:outputNumber value="{!v.campingListItem.Quantity__c}"/>
    </p>
    <p>price:
        <ui:outputNumber value="{!v.campingListItem.Price__c}"/>
    </p>
    <p>Packed?:
        <ui:outputCheckbox value="{!v.campingListItem.Packed__c}"/>
    </p>
</aura:component>